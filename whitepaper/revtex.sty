      \uppercase{\vspace{16pt}Appendix\ifx\one\empty\else: \fi}%
      \global\nonumberfalse%
    \else%
      \uppercase{%
        \vspace{16pt}Appendix \appletter\ifx\one\empty\else: \fi}%
    \fi%
    \uppercase{#1}%
    }%
  \global\c@secnum=0%
  \edef\@currentlabel{\appletter}\vskip12pt%
