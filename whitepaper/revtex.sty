\def\@collapse#1{%
{% grouping to allow use of latex temps
\sfcode`,=2000\relax
\dimen0=\the\fontdimen2\textfont0\relax
\xspaceskip = .6\dimen0 plus.3\dimen0 minus.3\dimen0\relax
\let\@temp\relax
\@tempcntb\@MM
